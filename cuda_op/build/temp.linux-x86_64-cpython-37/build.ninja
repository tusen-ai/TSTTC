ninja_required_version = 1.3
cxx = c++
nvcc = /usr/local/cuda/bin/nvcc

cflags = -pthread -B /root/anaconda3/envs/ourttc/compiler_compat -Wl,--sysroot=/ -Wsign-compare -DNDEBUG -g -fwrapv -O3 -Wall -Wstrict-prototypes -fPIC -I/root/anaconda3/envs/ourttc/lib/python3.7/site-packages/torch/include -I/root/anaconda3/envs/ourttc/lib/python3.7/site-packages/torch/include/torch/csrc/api/include -I/root/anaconda3/envs/ourttc/lib/python3.7/site-packages/torch/include/TH -I/root/anaconda3/envs/ourttc/lib/python3.7/site-packages/torch/include/THC -I/usr/local/cuda/include -I/root/anaconda3/envs/ourttc/include/python3.7m -c
post_cflags = -DTORCH_API_INCLUDE_EXTENSION_H '-DPYBIND11_COMPILER_TYPE="_gcc"' '-DPYBIND11_STDLIB="_libstdcpp"' '-DPYBIND11_BUILD_ABI="_cxxabi1011"' -DTORCH_EXTENSION_NAME=custom_gridsample -D_GLIBCXX_USE_CXX11_ABI=0 -std=c++14
cuda_cflags = -I/root/anaconda3/envs/ourttc/lib/python3.7/site-packages/torch/include -I/root/anaconda3/envs/ourttc/lib/python3.7/site-packages/torch/include/torch/csrc/api/include -I/root/anaconda3/envs/ourttc/lib/python3.7/site-packages/torch/include/TH -I/root/anaconda3/envs/ourttc/lib/python3.7/site-packages/torch/include/THC -I/usr/local/cuda/include -I/root/anaconda3/envs/ourttc/include/python3.7m -c
cuda_post_cflags = -D__CUDA_NO_HALF_OPERATORS__ -D__CUDA_NO_HALF_CONVERSIONS__ -D__CUDA_NO_BFLOAT16_CONVERSIONS__ -D__CUDA_NO_HALF2_OPERATORS__ --expt-relaxed-constexpr --compiler-options ''"'"'-fPIC'"'"'' -DTORCH_API_INCLUDE_EXTENSION_H '-DPYBIND11_COMPILER_TYPE="_gcc"' '-DPYBIND11_STDLIB="_libstdcpp"' '-DPYBIND11_BUILD_ABI="_cxxabi1011"' -DTORCH_EXTENSION_NAME=custom_gridsample -D_GLIBCXX_USE_CXX11_ABI=0 -gencode=arch=compute_86,code=compute_86 -gencode=arch=compute_86,code=sm_86 -std=c++14
cuda_dlink_post_cflags = 
ldflags = 

rule compile
  command = $cxx -MMD -MF $out.d $cflags -c $in -o $out $post_cflags
  depfile = $out.d
  deps = gcc

rule cuda_compile
  depfile = $out.d
  deps = gcc
  command = $nvcc  $cuda_cflags -c $in -o $out $cuda_post_cflags





build /root/ourTTC/cuda_op/build/temp.linux-x86_64-cpython-37/grid_sample.o: compile /root/ourTTC/cuda_op/grid_sample.cpp
build /root/ourTTC/cuda_op/build/temp.linux-x86_64-cpython-37/grid_sample_kernel.o: cuda_compile /root/ourTTC/cuda_op/grid_sample_kernel.cu







